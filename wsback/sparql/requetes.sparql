PREFIX ex: <http://www.semanticweb.org/msi/ontologies/2025/8/bookshare#>

SELECT ?event ?nomEvenement ?description ?lieu ?dateDebut ?dateFin
WHERE {
  ?event rdf:type ex:Evenement ;
         ex:nomEvenement ?nomEvenement ;
         ex:description_event ?description ;
         ex:lieu ?lieu ;
         ex:dateDebut ?dateDebut ;
         ex:dateFin ?dateFin .
         
  FILTER(?nomEvenement = "Atelier Recyclage Papier et Plastique")
}


