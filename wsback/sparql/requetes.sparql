# 1️⃣ Tous les individus et leurs classes
##############################################
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX ex: <http://www.semanticweb.org/msi/ontologies/2025/8/bookshare#>

SELECT DISTINCT ?individu ?classe ?label
WHERE {
  ?individu rdf:type ?classe .
  OPTIONAL { ?individu rdfs:label ?label . }
  FILTER(!isBlank(?individu))
}
LIMIT 100


PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
