SELECT ?point ?capacity ?location ?status ?typeConteneur ?label
WHERE {
  ?point rdf:type ex:Point_de_collecte ;
         ex:capacity_point_de_collecte ?capacity ;
         ex:location_point_de_collecte ?location ;
         ex:status_point_de_collecte ?status ;
         ex:typeConteneur ?typeConteneur .
         
  OPTIONAL { ?point rdfs:label ?label }
}

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
