SELECT ?Centre_de_recyclage ?centerName ?capacity ?energyConsumption ?location ?openingDate ?status ?recyclingRate
WHERE {
  ?Centre_de_recyclage rdf:type ex:Centre_de_recyclage ;
          ex:centerName ?centerName ;
          ex:capacity_center ?capacity ;
          ex:energyConsumption ?energyConsumption ;
          ex:location_center ?location ;
          ex:openingDate ?openingDate ;
          ex:operationStatus ?status ;
          ex:recyclingRate ?recyclingRate .
}



