case "$(uname)" in
    CYGWIN*|MSYS*|MINGW*)
        # transform D:\path\to\venv to /d/path/to/venv on MSYS and MINGW
        # and to /cygdrive/d/path/to/venv on Cygwin
        VIRTUAL_ENV=$(cygpath 'C:\Users\ASUS\Desktop\web semantique\WS\wsback\venv')
        export VIRTUAL_ENV
        ;;
    *)
        # use the path as-is
        export VIRTUAL_ENV='C:\Users\ASUS\Desktop\web semantique\WS\wsback\venv'
        ;;
esac

_OLD_VIRTUAL_PATH="$PATH"
PATH="$VIRTUAL_ENV/"Scripts":$PATH"
export PATH

VIRTUAL_ENV_PROMPT='(venv) '
export VIRTUAL_ENV_PROMPT

